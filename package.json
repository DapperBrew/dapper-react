{
  "name": "dapper-react",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "7.7.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "concurrently": "^3.3.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^1.0.0",
    "node-sass": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "classnames": "^2.2.5",
    "dapper-calc": "github:dapperbrew/DapperCalc",
    "history": "^4.6.1",
    "lodash": "^4.17.3",
    "match-sorter": "^1.6.0",
    "normalize-scss": "^7.0.0",
    "normalize.scss": "^0.1.0",
    "normalizr": "^3.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-modal": "^1.6.5",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-rte": "^0.11.0",
    "react-scripts": "1.0.7",
    "react-search-input": "^0.11.2",
    "react-select": "^1.0.0-rc.2",
    "react-table": "^6.0.5",
    "react-toggle": "^4.0.1",
    "react-tooltip": "^3.2.7",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-persist": "^4.3.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
    "susy": "^2.2.12",
    "uuid": "^3.0.1",
    "validator": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --kill-others --names 'react-scripts,sass' --prefix name 'react-scripts start' 'npm run styles:watch'",
    "dev": "npm run watch",
    "build": "npm run styles & react-scripts build",
    "eject": "react-scripts eject",
    "styles": "node-sass --include-path ./node_modules ./src/scss/style.scss -o ./src/css",
    "styles:watch": "node-sass --include-path ./node_modules/ ./src/scss/style.scss -w  -o ./src/css",
    "firebase": "npm run build && firebase deploy",
    "test": "./node_modules/.bin/eslint src; exit 0",
    "stage": "now -e DB_HOST=@host -e REACT_APP_API_URL=@api",
    "deploy": "now -e DB_HOST=@host -e REACT_APP_API_URL=@api && now alias"
  },
  "now": {
    "alias": "dapperbrew.com"
  }
}
