{
  "name": "dapper-react",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "concurrently": "^3.1.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "gh-pages": "^0.12.0",
    "node-sass": "^4.1.0",
    "normalize.scss": "^0.1.0",
    "react-scripts": "0.8.4"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "classnames": "^2.2.5",
    "dapper-calc": "github:dapperbrew/DapperCalc",
    "lodash": "^4.17.3",
    "normalize-scss": "^6.0.0",
    "normalizr": "^3.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-modal": "^1.6.4",
    "react-redux": "^5.0.1",
    "react-router": "^4.0.0-beta.3",
    "react-router-dom": "^4.0.0-beta.4",
    "react-search-input": "^0.10.3",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-persist": "^4.3.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "susy": "^2.2.12"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --kill-others --names 'react-scripts,sass' --prefix name 'react-scripts start' 'npm run styles:watch'",
    "build": "npm run styles & react-scripts build",
    "eject": "react-scripts eject",
    "styles": "node-sass --include-path ./node_modules ./src/scss/style.scss -o ./src/css",
    "styles:watch": "node-sass --include-path ./node_modules/ ./src/scss/style.scss -w  -o ./src/css",
    "deploy": "git push heroku master",
    "test": "./node_modules/.bin/eslint src"
  }
}
