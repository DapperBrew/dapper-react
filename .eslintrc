{
   "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },

  "extends": "airbnb",

  "parser": "babel-eslint",

  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 0,
    "react/jsx-boolean-value": 0,
  },
}
